<!-- Lets talk about geddy -->
<div class="hero-unit geddy-welcome-box">
  <% if (user) { %>
    <h1>
      Hello<% if (user.givenName) { %>, <%= user.givenName %><% } %>
    </h1>
    <p>You've successfully logged in to the Coalition for the Liberation of Itinerant Tree-Dwellers!(via <%= authType %>).</p>
    <p><a href="/logout">Click here to log back out.</a></p>
  <% } else { %>
    <h1>Hello, stranger</h1>
    <p> Join the Coalition for the Liberation of Itinerant Tree-Dwellers!. </p>
    <br>
    <p><a href="/users/add">Click here to create a user account.</a></p>
    <p><a href="/login">Click here to log in with Facebook or Twitter.</a></p>
    <br>
    <p>Been here before???? <a href="/login"> Login Back In.</a></p>
  <% } %>
  <br />
  <img src="/img/santadancers.gif" />
</div>

